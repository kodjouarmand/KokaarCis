@model LandTitleViewModel

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    var title = "Visualisation d'un titre foncier";
}
<div class="row p-1 bg-info bg-gradient">
    <h4 class="text-white"><partial name="_IconHeader" />&nbsp;@title</h4>
</div>
<div id="parent-view">
    @await Html.PartialAsync("_Detail", Model)
</div>
<div id="modal-view">
    @await Html.PartialAsync("_Modal", Model)
</div>


@section Scripts{
    <script src="~/js/landTitle.js"></script>    
}


